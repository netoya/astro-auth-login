{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@functions/*": ["src/functions/*"],
      "@modules/*": ["src/modules/*"],
      "@common/*": ["src/modules/common/*"],
      "@auth/*": ["src/modules/auth/*"],
      "@home/*": ["src/modules/home/*"]
    }
  },  
}